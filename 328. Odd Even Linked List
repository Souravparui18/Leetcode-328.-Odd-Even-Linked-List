class Solution {
    public ListNode oddEvenList(ListNode head) {
        if(head == null || head.next == null){
            return head;
        }
        ListNode c1 = head;
        ListNode c2 = c1.next;
        ListNode temp = c2;

        while(c2 != null && c2.next != null){
            c1.next = c2.next;
            c1 = c1.next;
            c2.next = c1.next;
            c2 = c2.next;
        }
        c1.next = temp;
        return head;
    }
}
